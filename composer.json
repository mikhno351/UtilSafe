{
  "name": "ket-php/utils-safe",
  "description": "Lightweight PHP utility for safely accessing variables, handling undefined values without errors.",
  "keywords": [
    "php",
    "safe",
    "utility",
    "helpers",
    "error-handling",
    "undefined",
    "null-safe",
    "ketphp",
    "ket-php"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "KetPHP\\Utils\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Alexander Mikhno",
      "email": "mikhno351@yandex.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "suggest": {
    "ext-mbstring": "Recommended for multibyte string safety"
  },
  "prefer-stable": true,
  "homepage": "https://github.com/mikhno351/KetPHP-Safe",
  "support": {
    "issues": "https://github.com/mikhno351/KetPHP-Safe/issues",
    "source": "https://github.com/mikhno351/KetPHP-Safe"
  }
}
